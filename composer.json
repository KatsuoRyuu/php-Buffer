{
    "name": "kryuu-common/buffer",
    "description": "Buffer controller for multiple types of conversions like Base64 and Base58",
    "repositories": [ 
        { "type": "composer", "url": "https://composer.ryuu.technology/packages/php/" }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "kryuu-common/base58": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KryuuCommon\\Buffer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KryuuCommon\\BufferTest\\":   "test/"
        }
    }
}
